digraph key {
  nodesep=0.01;
  rankdir="LR";
  node [style=invis, shape=none, fixedsize=true, width=0, label=""];

  replication_color_from -> replication_color [color="#880000"];
  replication_color [style=solid, fixedsize=false, label="Replication Queue", fontcolor="#880000"];

  job_color_from -> job_color [color="#8800cc"];
  job_color [style=solid, fixedsize=false, label="Job Queue", fontcolor="#8800cc"];

  web_color_from -> web_color;
  web_color [style=solid, fixedsize=false, label="Web traffic"];

  fancy_from -> fancy [arrowhead=diamond];
  fancy [style=solid, fixedsize=false, label="Fancy routing\nConsistent hashing or\notherwise intelligent"];

  disabled_from -> disabled [style=dashed];
  disabled [style=solid, fixedsize=false, label="Theoretical\nCurrently disabled"];

  noraml_from -> normal;
  normal [style=solid, fixedsize=false, label="Normal load balancing\nLVS and round robin"];
}